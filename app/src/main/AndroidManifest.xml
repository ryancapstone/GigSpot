<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.gigspot">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".LoginRegistrationCode.LoginActivity"
            android:theme="@style/NavTheme">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <activity android:name=".LoginRegistrationCode.RegisterActivity"
            android:theme="@style/NavTheme"/>

        <activity android:name=".LoginRegistrationCode.Forgot_EmailPw_Activity"/>

        <activity android:name=".LoginRegistrationCode.VerifyActivity"/>
    </application>

</manifest>